(defun lupan-themes-create-dark-theme (theme bg)
  (cl-labels ((base (hue) (lupan-themes-hsl-to-hex hue 0.25 0.7))
	      (dark1 (hue) (lupan-themes-hsl-to-hex hue 0.14 0.5))
	      (dark2 (hue) (lupan-themes-hsl-to-hex hue 0.25 0.4))
	      (dark3 (hue) (lupan-themes-hsl-to-hex hue 0.25 0.3))
	      (grayish (hue) (lupan-themes-hsl-to-hex hue 0.07 0.7))
	      (dark1-grayish (hue) (lupan-themes-hsl-to-hex hue 0.07 0.5))
	      (dark2-grayish (hue) (lupan-themes-hsl-to-hex hue 0.07 0.4))
	      (dark3-grayish (hue) (lupan-themes-hsl-to-hex hue 0.07 0.3)))
    (let ((black "#000000")
	  (fg (lupan-themes-hsl-to-hex bg 0.07 0.8))
	  (selected bg)
	  (light-gray (lupan-themes-hsl-to-hex 0.0 0.0 0.8))
	  (dark1-gray (lupan-themes-hsl-to-hex 0.0 0.0 0.5))
	  (dark2-gray (lupan-themes-hsl-to-hex 0.0 0.0 0.4))
	  (dark3-gray (lupan-themes-hsl-to-hex 0.0 0.0 0.3))
	  (orange (base 'orange))
	  (yellow (base 'yellow))
	  (green (base 'green))
	  (cyan (base 'cyan))
	  (blue (base 'blue))
	  (slate-blue (base 'SlateBlue2))
	  (magenta (base 'magenta))
	  (red (base 'red))
	  (dark1-yellow (dark1 'yellow))
	  (dark1-green (dark1 'green))
	  (dark1-slate-blue (dark1 'SlateBlue2))
	  (dark1-indigo (dark1 'indigo))
	  (dark1-red (dark1 'red))
	  (dark1-blue-gray (dark1-grayish 'blue))
	  (dark1-cyan-gray (dark1-grayish 'cyan))
